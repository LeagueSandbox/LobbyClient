{
  "name": "leaguesandbox-lobbyclient",
  "version": "0.0.1",
  "description": "Lobby client for the League Sandbox project.",
  "main": "src/browser/main.js",
  "scripts": {
    "start": "./node_modules/.bin/electron . --dev",
    "watch": "./node_modules/.bin/webpack-dev-server --hot --inline",
    "bundle": "./node_modules/.bin/webpack --bail",
    "clean": "rm -r built src/built",
    "package:osx": "./node_modules/.bin/electron-packager . LobbyClient --out=built --platform=darwin --arch=x64 --version=0.37.5 --ignore='(node_modules|src/ui/components|src/ui/directives|src/ui/css|src/ui/services)'",
    "package:win": "./node_modules/.bin/electron-packager . LobbyClient --out=built --platform=win32 --arch=x64 --version=0.37.5 --ignore='(node_modules|src/ui/components|src/ui/directives|src/ui/css|src/ui/services)'",
    "test": "npm run bundle"
  },
  "keywords": [
    "intwars",
    "client",
    "lol",
    "league",
    "of",
    "legends"
  ],
  "author": "molenzwiebel",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.7.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "bluebird": "^3.3.5",
    "css-loader": "^0.23.1",
    "electron-packager": "^7.0.0",
    "electron-prebuilt": "^0.37.5",
    "file-loader": "^0.8.5",
    "less": "^2.6.1",
    "less-loader": "^2.2.3",
    "raw-loader": "^0.5.1",
    "style-loader": "^0.13.1",
    "ts-loader": "^0.8.2",
    "typescript": "^1.8.10",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1",
    "webpack-fail-plugin": "^1.0.4"
  },
  "dependencies": {
    "angular": "^1.5.3",
    "angular-animate": "^1.5.5",
    "angular-route": "^1.5.3",
    "angularjs-scroll-glue": "^2.0.4",
    "babel-polyfill": "^6.7.4",
    "bluebird": "^3.3.4",
    "font-awesome": "^4.5.0",
    "socket.io-client": "^1.4.5"
  }
}
